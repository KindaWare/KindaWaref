            --mattts UI
            SwScripts:NewButton("Arsenal Autokill", "autokill", function()

                game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(k)
                    if k == "e" then
                    local Gun = game.ReplicatedStorage.Weapons:FindFirstChild(game.Players.LocalPlayer.NRPBS.EquippedTool.Value);
                    local Crit = math.random() > .6 and true or false;
                    for i,v in pairs(game.Players:GetPlayers()) do
                    if v and v.Character and v.Character:FindFirstChild("Head") then
                    local Distance = (game.Players.LocalPlayer.Character.Head.Position - v.Character.Head.Position).magnitude
                    for i  = 1,10 do
                    game.ReplicatedStorage.Events.HitPart:FireServer(v.Character.Head,
                    v.Character.Head.Position + Vector3.new(math.random(), math.random(), math.random()),
                    Gun.Name,
                    Crit and 2 or 1,
                    Distance,
                    Backstab,
                    Crit,
                    false,
                    1,
                    false,
                    Gun.FireRate.Value,
                    Gun.ReloadTime.Value,
                    Gun.Ammo.Value,
                    Gun.StoredAmmo.Value,
                    Gun.Bullets.Value,
                    Gun.EquipTime.Value,
                    Gun.RecoilControl.Value,
                    Gun.Auto.Value,
                    Gun['Speed%'].Value,
                    game.ReplicatedStorage.wkspc.DistributedTime.Value);
                    end
                    end
                    end
                    end
                    end)
                    
                end)

                --Script Analyser
                SwScripts:NewButton("Script Analyser", "Analyze Scripts", function()

                    loadstring(game:HttpGet("https://raw.githubusercontent.com/debug420/rbx-script-analyzer/main/Script-Analyzer.lua"))()
                        
                    end)


                    --OrHub
                    SwScripts:NewButton("OrHub", "Allows for amazing FE dances and animations across any game.", function()

                        loadstring(game:HttpGet("https://pastebin.com/raw/khS8BvvK"))()
                            
                        end)


                        --Dex V4
                        SwScripts:NewButton("Dex", "Sadly i cant find it", function()

                            loadstring(game:HttpGetAsync("https://pastebin.com/raw/fPP8bZ8Z"))()
                                
                            end)


                            
                                SwScripts:NewButton("Solaris Hub", "Amazing Script", function()
    
                                    loadstring(game:HttpGet('https://solarishub.dev/script.lua',true))()
                                    
                                end)
-- Universal
Universal:NewSlider("Walkspeed", "Changes your walkspeed magnitude.", 250, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Universal:NewSlider("JumpPower", "Changes your JumpPower magnitude.", 250, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
Universal:NewToggle("Noclip", "Noclips the localplayer.", function(state)
getgenv().noclip = state
end)
Universal:NewToggle("Godmode", "Auto respawns", function(state)
getgenv().godmode = state
end)
